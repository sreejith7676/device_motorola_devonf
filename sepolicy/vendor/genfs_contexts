# Fingerprint
genfscon sysfs /devices/virtual/fingerprint u:object_r:vendor_sysfs_fingerprint:s0

# Extcon
genfscon sysfs /devices/platform/soc/soc:extcon_usb/extcon/extcon0/cable.0/name  u:object_r:sysfs_extcon:s0
genfscon sysfs /devices/platform/soc/soc:extcon_usb/extcon/extcon0/cable.1/name  u:object_r:sysfs_extcon:s0

# Input Devices
genfscon sysfs /devices/virtual/input u:object_r:vendor_sysfs_input:s0

# Motorola Specific filesystem entries
genfscon proc /bootinfo u:object_r:vendor_proc_hw:s0
genfscon proc /config u:object_r:vendor_proc_hw:s0
genfscon proc /hw u:object_r:vendor_proc_hw:s0

# Power Supply
genfscon sysfs /devices/platform/11ed4000.i2c/i2c-9/9-0066/power_supply/cp-standalone u:object_r:sysfs_power_supply:s0
genfscon sysfs /devices/platform/charger/power_supply/mtk-master-charger u:object_r:sysfs_power_supply:s0

# Touchpanel
genfscon sysfs /class/touchscreen u:object_r:vendor_sysfs_touchpanel:s0
genfscon sysfs /devices/virtual/touchscreen u:object_r:vendor_sysfs_touchpanel:s0

